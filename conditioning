function App() {
  const [isShown, setisShown] = React.useState(false)
  
  function toggle() {
    
    setisShown(prevsetisShown => !prevsetisShown)
    alert(isShown)
    
  }

  return (
    <div>
      <button onClick={toggle}></button>
      {isShown && <h1>hgfhgfhfg</h1>}
      


    </div>
  )


}
  






export default App;

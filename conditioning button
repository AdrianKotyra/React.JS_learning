function App() {
  const [isShown, setisShown] = React.useState(true)

  function clearMessages() {
    setisShown(prevMessages=>
      !prevMessages)
      

  }

  return (
    <div>
      <button onClick={clearMessages}></button>

      {isShown&& <h1>Show punchline</h1>}
      {!isShown && <h1>hide punchline</h1>}


    </div>
  )




}

export default App;
